@model  TCC_2._0.Models.PROTIPO


@{
    ViewBag.Title = "Atualizar";
}

<h2>Atualizando Estoque</h2>

<div>
    <form action="/ProTipo/Atualizar?id=@Model.PTID" method="post">




        

        <div>
            <class ="form-group">
                <label for="estoque">Estoque</label>
                <input type="number" class="form-control"
                       id="estoque" name="estoque" min="0"
                       value="@Model.PTESTOQUE"
                       required />
        </div>

       


        <div>



            <div>
                <button type="submit" class="btn btn-success">Salvar</button>
                <a href="/protipo/Alerta" class="btn btn-primary">Voltar</a>

            </div>


        </div>
    </form>
</div>
